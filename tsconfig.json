{
    "compilerOptions": {
        "declaration": true,
        "target": "es5",
        "outDir": "./dist",
        "listEmittedFiles": true,
        "module": "commonjs",
        "noEmitOnError": true,
        "skipLibCheck": true,
        "lib": [
            "es2015"
        ],
        "types": [
            "node"
        ]
    },
    "exclude": [
        "node_modules",
        "dist",
        "**/__tests__/*.*"
    ]
}